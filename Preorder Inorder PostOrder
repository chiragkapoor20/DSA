
void preorder(Node* root){
if(!root) return ;

cout<<root->data;
preorder(root->left);
preorder(root->right);


}



void iorder(Node* root){
if(!root) return ;

inorder(root->left);
cout<<root->data;
inorder(root->right);


}



void postorder(Node* root){
if(!root) return ;

postrder(root->left);
postrder(root->right);
cout<<root->data;


}

